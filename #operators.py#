#Arithmetic operators

num1=34
num2=10

print(num1 + num2)
print(num1 - num2)
print(num1 * num2)
print(num1 / num2)
print(num1 % num2) #modulus operator returns a reminder of a division

#Comparison operators

print(num1<num2)
print(num1>num2)
print(num1<=num2)
print(num1>=num2)
print(num1==num2)
print(num1!=num2)

#Assignment operators
w=7
print(w)

w +=3
print(w)

b=7
b+=2
print(b)

#Logical operators - and, or, not

print(56<100 and 45>23)
print(56<100 or(56<100 or 45>23) 45>23)
print(not